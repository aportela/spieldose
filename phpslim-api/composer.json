{
    "name": "aportela/spieldose",
    "description": "an application for streaming music hosted on your own server",
    "type": "project",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "alex",
            "email": "766f6964+github@gmail.com"
        }
    ],
    "homepage": "https://github.com/aportela/spieldose",
    "config": {
        "sort-packages": true,
        "process-timeout": 0
    },
    "require": {
        "php": ">=8.0",
        "aportela/db-browser-wrapper": "^1.0",
        "aportela/db-wrapper": "^2.0",
        "aportela/lastfm-wrapper": "^1.0",
        "aportela/mediawiki-wrapper": "^1.0",
        "aportela/musicbrainz-wrapper": "^2.0",
        "aportela/remote-thumbnail-cache-wrapper": "^2.0",
        "firebase/php-jwt": "^6.8",
        "james-heinrich/getid3": "^1.9",
        "monolog/monolog": "^3.4",
        "php-di/php-di": "^7.0",
        "ramsey/uuid": "^4.7",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.12",
        "slim/twig-view": "^3.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.22",
        "phpunit/phpunit": "^10.3"
    },
    "autoload": {
        "psr-4": {
            "Spieldose\\": "Spieldose"
        }
    },
    "scripts": {
        "fix" : "vendor/bin/php-cs-fixer fix Spieldose",
        "start": "php -S 127.0.0.1:8081 -t public public/cliserver.php",
        "test": "vendor/bin/phpunit"
    }
}
